{
    "presets": ["@babel/preset-env", "@babel/preset-react"],
    "plugins": [
        ["@babel/plugin-proposal-decorators", { "legacy": true }],
        "@babel/plugin-transform-class-properties",
        "@babel/plugin-syntax-dynamic-import",
        [
            "module-resolver",
            {
                "root": [
                    "./src/javascript"
                ],
                "alias": {
                    "_common"  : "./src/javascript/_common",
                    "Images"   : "./src/images/"
                }
            }
        ]
    ]
}
