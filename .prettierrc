{
    "singleQuote": true,
    "tabWidth": 4,
    "semi": true,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "printWidth": 100,
    "endOfLine": "lf",
    "object-property-newline": "always",
    "key-spacing": {
        "beforeColon": false,
        "afterColon": true,
        "mode": "strict"
    }
}